            <rowConstraints>
                <RowConstraints percentHeight="50.0" vgrow="NEVER" />
                <RowConstraints percentHeight="50.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Text fx:id="qty" strokeType="OUTSIDE" strokeWidth="0.0" text="2x" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowSpan="2" GridPane.valignment="CENTER">
