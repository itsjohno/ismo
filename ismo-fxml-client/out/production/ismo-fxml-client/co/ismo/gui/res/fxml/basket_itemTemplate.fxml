                    <children>
                        <Text fx:id="barcode" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-weight: bold;" text="5054402420930">
                            <HBox.margin>
                                <Insets right="10.0" />
                            </HBox.margin>
                        </Text>
                        <Text fx:id="productName" strokeType="OUTSIDE" strokeWidth="0.0" text="Adventure Time - Volume Six" />
                    </children>
                    <padding>
                        <Insets left="10.0" />
                    </padding>
                </HBox>
                <HBox alignment="CENTER_LEFT" focusTraversable="false" prefHeight="100.0" prefWidth="200.0" GridPane.rowIndex="1">
                    <children>
                        <Text fx:id="supercat" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-weight: bold;" text="Books">
                            <HBox.margin>
                                <Insets right="5.0" />
                            </HBox.margin>
                        </Text>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="/">
                            <HBox.margin>
                                <Insets right="5.0" />
                            </HBox.margin>
                        </Text>
                        <Text fx:id="subcat" strokeType="OUTSIDE" strokeWidth="0.0" text="Graphic Novels &amp; Comics" />
                    </children>
                    <GridPane.margin>
                        <Insets />
