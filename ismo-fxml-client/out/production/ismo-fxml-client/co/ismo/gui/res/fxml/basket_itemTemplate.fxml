                        <Font size="16.0" />
                    </font>
                </Text>
                <Text fx:id="qtyValue" fill="RED" strokeType="OUTSIDE" strokeWidth="0.0" text="£201.00" GridPane.columnIndex="3" GridPane.halignment="CENTER" GridPane.rowSpan="2" GridPane.valignment="CENTER">
