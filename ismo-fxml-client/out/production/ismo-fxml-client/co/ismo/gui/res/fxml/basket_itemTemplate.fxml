                </Text>
                <Text fx:id="value" strokeType="OUTSIDE" strokeWidth="0.0" text="£100.50" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.rowSpan="2" GridPane.valignment="CENTER">
