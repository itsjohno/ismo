<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" hgap="15.0" vgap="15.0" AnchorPane.bottomAnchor="0.0"
          AnchorPane.leftAnchor="0.0"
          AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"
          fx:controller="co.ismo.gui.controller.ButtonController" xmlns="http://javafx.com/javafx/8">
    <columnConstraints>
        <ColumnConstraints percentWidth="50.0"/>
        <ColumnConstraints percentWidth="50.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="20.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="20.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="20.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="20.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="20.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="stdTillBtn"
                wrapText="true" GridPane.columnIndex="0" GridPane.rowIndex="0" focusTraversable="false"
                onAction="#productLookup">
            <graphic>
                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                    <children>
                        <Text styleClass="stdTillBtnTxt" text="Product Lookup"/>
                        <Text styleClass="stdTillBtnKey" text="F1"/>
                    </children>
                </VBox>
            </graphic>
        </Button>
        <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="stdTillBtn"
                GridPane.columnIndex="1" GridPane.rowIndex="0" focusTraversable="false" onAction="#notImplemented" disable="true">
            <graphic>
                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                    <children>
                        <Text styleClass="stdTillBtnTxt" text="Trade In"/>
                        <Text styleClass="stdTillBtnKey" text="F2"/>
                    </children>
                </VBox>
            </graphic>
        </Button>
        <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="stdTillBtn"
                GridPane.columnIndex="0" GridPane.rowIndex="1" focusTraversable="false" onAction="#notImplemented" disable="true">
            <graphic>
                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                    <children>
                        <Text styleClass="stdTillBtnTxt" text="Return"/>
                        <Text styleClass="stdTillBtnKey" text="F3"/>
                    </children>
                </VBox>
            </graphic>
        </Button>
        <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="stdTillBtn"
                GridPane.columnIndex="1" GridPane.rowIndex="1" focusTraversable="false" onAction="#notImplemented" disable="true">
            <graphic>
                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                    <children>
                        <Text styleClass="stdTillBtnTxt" text="Discount"/>
                        <Text styleClass="stdTillBtnKey" text="F4"/>
                    </children>
                </VBox>
            </graphic>
        </Button>
        <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="stdTillBtn"
                GridPane.columnIndex="0" GridPane.rowIndex="2" focusTraversable="false" onAction="#suspendTransaction" fx:id="suspendButton">
            <graphic>
                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                    <children>
                        <Text styleClass="stdTillBtnTxt" text="Suspend"/>
                        <Text styleClass="stdTillBtnKey" text="F5"/>
                    </children>
                </VBox>
            </graphic>
        </Button>
        <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="stdTillBtn"
                GridPane.columnIndex="1" GridPane.rowIndex="2" focusTraversable="false" onAction="#resumeTransaction" fx:id="resumeButton">
            <graphic>
                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                    <children>
                        <Text styleClass="stdTillBtnTxt" text="Resume"/>
                        <Text styleClass="stdTillBtnKey" text="F6"/>
                    </children>
                </VBox>
            </graphic>
        </Button>
        <Button maxHeight="1.7976931348623157E308" maxWidth="2.7976931348623157E308" styleClass="stdTillBtn"
                GridPane.columnIndex="0" GridPane.rowIndex="3" focusTraversable="false" onAction="#voidItem">
            <graphic>
                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                    <children>
                        <Text styleClass="stdTillBtnTxt" text="Void Item"/>
                        <Text styleClass="stdTillBtnKey" text="F7"/>
                    </children>
                </VBox>
            </graphic>
        </Button>
        <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="stdTillBtn"
                GridPane.columnIndex="1" GridPane.rowIndex="3" focusTraversable="false" onAction="#notImplemented" disable="true">
            <graphic>
                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                    <children>
                        <Text styleClass="stdTillBtnTxt" text="No Sale"/>
                        <Text styleClass="stdTillBtnKey" text="F8"/>
                    </children>
                </VBox>
            </graphic>
        </Button>
        <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="stdTillBtn"
                GridPane.columnIndex="0" GridPane.rowIndex="4" focusTraversable="false" onAction="#webBrowser">
            <graphic>
                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                    <children>
                        <Text styleClass="stdTillBtnTxt" text="Web Browser"/>
                        <Text styleClass="stdTillBtnKey" text="F9"/>
                    </children>
                </VBox>
            </graphic>
        </Button>
        <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="stdTillBtn"
                GridPane.columnIndex="1" GridPane.rowIndex="4" focusTraversable="false" onAction="#notImplemented" disable="true">
            <graphic>
                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                    <children>
                        <Text styleClass="stdTillBtnTxt" text="Reports"/>
                        <Text styleClass="stdTillBtnKey" text="F10"/>
                    </children>
                </VBox>
            </graphic>
        </Button>
    </children>
</GridPane>
